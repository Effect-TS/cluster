{
  "exclude": ["src/internal/**/*.ts"],
  "theme": "mikearnaldi/just-the-docs",
  "parseCompilerOptions": {
    "strict": true,
    "noEmit": true,
    "target": "ES2021",
    "lib": ["ES2021"],
    "paths": {
      "@effect/shardcake": ["./src/index.ts"],
      "@effect/shardcake/test/*": ["./test/*"],
      "@effect/shardcake/examples/*": ["./examples/*"],
      "@effect/shardcake/*": ["./src/*"]
    }
  },
  "examplesCompilerOptions": {
    "strict": true,
    "noEmit": true,
    "target": "ES2021",
    "lib": ["ES2021"],
    "paths": {
      "@effect/shardcake": ["../../src/index.ts"],
      "@effect/shardcake/test/*": ["../../test/*"],
      "@effect/shardcake/examples/*": ["../../examples/*"],
      "@effect/shardcake/*": ["../../src/*"]
    }
  }
}
