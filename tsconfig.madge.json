{
  "compilerOptions": {
    "noEmit": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@effect/sharding/test/*": ["./packages/sharding/build/test/*"],
      "@effect/sharding/examples/*": ["./packages/sharding/build/examples/*"],
      "@effect/sharding/*": ["./packages/sharding/build/esm/*"],

      "@effect/sharding-node/test/*": ["./packages/sharding-node/build/test/*"],
      "@effect/sharding-node/examples/*": [
        "./packages/sharding-node/build/examples/*"
      ],
      "@effect/sharding-node/*": ["./packages/sharding-node/build/esm/*"]
    }
  },
  "include": [
    "./packages/sharding/build/esm/**/*.js",
    "./packages/sharding/build/test/**/*.js",
    "./packages/sharding/build/examples/**/*.js",

    "./packages/sharding-node/build/esm/**/*.js",
    "./packages/sharding-node/build/test/**/*.js",
    "./packages/sharding-node/build/examples/**/*.js"
  ]
}
