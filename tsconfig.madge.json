{
  "compilerOptions": {
    "noEmit": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@effect/cluster/test/*": [
        "./packages/cluster/build/test/*"
      ],
      "@effect/cluster/examples/*": [
        "./packages/cluster/build/examples/*"
      ],
      "@effect/cluster/*": [
        "./packages/cluster/build/esm/*"
      ],
      "@effect/cluster-node/test/*": [
        "./packages/cluster-node/build/test/*"
      ],
      "@effect/cluster-node/examples/*": [
        "./packages/cluster-node/build/examples/*"
      ],
      "@effect/cluster-node/*": [
        "./packages/cluster-node/build/esm/*"
      ]
    }
  },
  "include": [
    "./packages/cluster/build/esm/**/*.js",
    "./packages/cluster/build/test/**/*.js",
    "./packages/cluster/build/examples/**/*.js",
    "./packages/cluster-node/build/esm/**/*.js",
    "./packages/cluster-node/build/test/**/*.js",
    "./packages/cluster-node/build/examples/**/*.js"
  ]
}
