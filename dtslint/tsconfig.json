{
  "compilerOptions": {
    "noEmit": true,
    "strict": true,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noErrorTruncation": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "ES2021",
    "lib": [
      "ES2021"
    ],
    "paths": {
      "@effect/shardcake": [
        "../src/index.ts"
      ],
      "@effect/shardcake/test/*": [
        "../test/*"
      ],
      "@effect/shardcake/examples/*": [
        "../examples/*"
      ],
      "@effect/shardcake/*": [
        "../src/*"
      ]
    }
  }
}
