{
  "include": [],
  "compilerOptions": {
    "strict": true,
    "moduleDetection": "force",
    "composite": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "esModuleInterop": false,
    "declaration": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "NodeNext",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "isolatedModules": true,
    "sourceMap": true,
    "declarationMap": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": false,
    "noErrorTruncation": false,
    "allowJs": false,
    "checkJs": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": false,
    "strictNullChecks": true,
    "baseUrl": ".",
    "target": "ES2022",
    "module": "NodeNext",
    "incremental": true,
    "removeComments": false,
    "paths": {
      "@effect/cluster/test/*": [
        "./packages/cluster/test/*.js"
      ],
      "@effect/cluster/*": [
        "./packages/cluster/src/*.js"
      ],
      "@effect/cluster-node/test/*": [
        "./packages/cluster-node/test/*.js"
      ],
      "@effect/cluster-node/*": [
        "./packages/cluster-node/src/*.js"
      ],
      "@effect/cluster-browser/test/*": [
        "./packages/cluster-browser/test/*.js"
      ],
      "@effect/cluster-browser/*": [
        "./packages/cluster-browser/src/*.js"
      ],
      "@effect/cluster-workflow/test/*": [
        "./packages/cluster-workflow/test/*.js"
      ],
      "@effect/cluster-workflow/*": [
        "./packages/cluster-workflow/src/*.js"
      ],
      "@effect/cluster-pg/test/*": [
        "./packages/cluster-pg/test/*.js"
      ],
      "@effect/cluster-pg/*": [
        "./packages/cluster-pg/src/*.js"
      ]
    },
    "plugins": [
      {
        "name": "@effect/language-service"
      }
    ]
  }
}
